// Data storage area over.

// -------------------------------------------------------------
// OBJECTS
// -------------------------------------------------------------

function Item(theContents, theTable){
 this.contents = theContents;
 this.tableID = theTable;

 // Functions
 this.get = itemGet;
 this.set = itemSet;
 this.create = itemCreate;
}

function itemCreate(theContents, theTable){
 this.contents = theContents;
 this.tableID = theTable;
}
function itemGet(){
 //alert("Get: "+this.tableID);
 if(this.tableID==undefined){
 return false;
 }
 return this.tableID;
}
function itemSet(theTable){
 this.tableID = theTable;
}

// -----------------------------------------

function Table(theName, thePhrase){
 this.name = theName;
 this.phrase = thePhrase;
 this.itemList = new Array();

 // Functions
 this.add = tableAdd;
 this.get = tableGet;

}

function tableGet(){
 result = "";
 total = this.itemList.length;
 //alert(total);
 rnd = Math.round(Math.random()*(total-1))+1;
 //alert(rnd);
 it = this.itemList[rnd-1];
 //alert("Generated: "+it.contents);
 result = this.phrase +" "+ it.contents;
 linkToTable = it.get();
 if(linkToTable){
  tb = master.getTable(linkToTable);
	if(tb){
	 if(tb.phrase==""){
	  result = result+tb.get();
	 }else{
	  result = result +" "+tb.get();
	 }
	 //alert(result);
	}
 }
 return result;
}

function tableAdd(item, tableID){
 i = new Item(item,tableID);
 //alert(i.get());
 this.itemList[this.itemList.length] = i;
}

// -------------------------------------------

function Master(){
 this.tables = new Array();

 // Functions
 this.addTable = masterAddTable;
 this.addTo = masterAddToTable;
 this.getTable = masterGetTable;
}

function masterAddTable(theName, thePhrase){
 t = new Table(theName, thePhrase);
 this.tables[this.tables.length] = t;
}

function masterAddToTable(table, item, tableID){
 t = this.getTable(table);
 if(t!=undefined){
  //alert("Chosen table for adding: "+t.name+" item:"+item);
	t.add(item,tableID);
 }
}

function masterGetTable(theName){
 theName = String(theName);
 for(i=0; i<this.tables.length; i++){
	t = this.tables[i];
	tName = String(t.name);
	//alert("Searching table: "+tName);
	if(tName.indexOf(theName)==0){
	 if(tName.length == theName.length){
	  //alert("Found table:"+tName);
	  return t;
	 }
	}
 }
 return false;
}

// ---------------------------------------------------------------
// Generic non-object functions
// ---------------------------------------------------------------

function makeTable(tName,tPhrase,check){
 master.addTable(tName,tPhrase);
 if(check == CHECK){
  useTables[useTables.length] = tName;
 }
 currentTable = tName;
 //alert(useTables);
}

function selectTable(tName){
 currentTable = tName;
}

function add(item,tableID){
 master.addTo(currentTable,item,tableID);
}

function genValue(){
 ret = "";
 for(j=0; j<useTables.length; j++){
	tName = useTables[j];
  //alert("NAME: "+tName);
	t = master.getTable(tName);
	ret += t.get();
 }
 document.x.itemData.value = ret;//b.substr(0,b.length-2);
 //alert("Backstory: "+ret);
}

//-->
</script>

<!--END SCRIPT -->

